UUID=5468fd36-556d-4e58-974c-e5d350584944  /  ext4  noatime,ro,errors=remount-ro  0  1
debugfs  /sys/kernel/debug  debugfs  defaults  0  0

tmpfs /var/log tmpfs defaults,noatime,size=5M 0 0
tmpfs /var/tmp tmpfs noatime,nosuid,nodev,mode=1777,size=5M 0 0
tmpfs /tmp tmpfs noatime,nosuid,nodev,mode=1777,,size=5M 0 0
tmpfs /var/spool tmpfs noatime,nosuid,nodev,mode=0775,size=1M 0 0
tmpfs /var/lib/dhcp tmpfs noatime,nosuid,nodev,mode=0775,size=1M 0 0

