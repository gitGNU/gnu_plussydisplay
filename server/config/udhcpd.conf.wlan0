start      192.168.4.100
end        192.168.4.200
interface  wlan0
max_leases 100
pidfile /var/run/udhcpd.wlan0.pid
lease_file /var/lib/misc/udhcpd.wlan0.leases
option subnet 255.255.255.0
option dns 192.168.4.1
option domain fellowship-franken.org
